<div class="subject" id="{{id}}">
  <div class="heading">
    <a class="top-link" href="#lens">^</a>
    <h4>{{absolutePath}}</h4>
  </div>
  <div class="body">
    <div class="column">
      <ul>
        <li>absolutePath: {{absolutePath}}</li>
        <li>apiLinks:
          {{#if apiLinks}}
            <ul class="{{listClass apiLinks}}">
              {{#each apiLinks}}
                <li><a href="{{this.href}}">{{this.method}}</a></li>
              {{/each}}
            </ul>
          {{/if}}
        </li>
        <li>childCount: {{childCount}}</li>
        <li>children:
          {{#if children}}
            <ul class="{{listClass children}}">
              {{#each children}}
                <li><a href="#{{this.id}}">{{this.name}}</a></li>
              {{/each}}
            </ul>
          {{/if}}
        </li>
        <li>createdAt: {{createdAt}}</li>
        <li>createdBy: {{createdBy.name}}</li>
        <li>description: {{description}}</li>
        <li>helpEmail: <a href="mailto:{{helpEmail}}">{{helpEmail}}</a></li>
        <li>helpUrl: <a href="{{helpUrl}}">{{helpUrl}}</a></li>
        <li>hierarchyLevel: {{hierarchyLevel}}</li>
        <li>id: {{id}}</li>
        <li>imageUrl: {{imageUrl}}</li>
        <li>isDeleted: {{isDeleted}}</li>
        <li>isPublished: {{isPublished}}</li>
        <li>name: {{name}}</li>
        <li>parentAbsolutePath: <a href="#{{parentId}}">{{parentAbsolutePath}}</a></li>
        <li>parentId: {{parentId}}</li>
        <li>relatedLinks:
          {{#if relatedLinks}}
            <ul class="{{listClass relatedLinks}}">
              {{#each relatedLinks}}
                <li><a href="{{this.url}}">{{this.name}}</a></li>
              {{/each}}
            </ul>
          {{/if}}
        </li>
        <li>tags:
          {{#if tags}}
            <ul class="{{listClass tags}}">
              {{#each tags}}
                <li>{{this}}</li>
              {{/each}}
            </ul>
          {{/if}}
        </li>
        <li>updatedAt: {{updatedAt}}</li>
      </ul>
    </div>
    {{#if samples}}
      <div class="column samples">
        <ul>
          {{#each samples}}
            <li>
              <a href="#{{this.id}}">{{this.aspect.name}}</a>:
              <span class="status-{{this.status}}">{{this.status}}</span>
            </li>
          {{/each}}
        </ul>
      </div>
    {{/if}}
  </div>
</div>
